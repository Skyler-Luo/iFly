# 环境变量配置示例文件
# 复制此文件为 .env.local 并根据实际情况修改

# API基础URL
VUE_APP_API_BASE_URL=http://127.0.0.1:8000/api

# 应用环境
VUE_APP_ENV=development

# 应用标题
VUE_APP_TITLE=iFly航空订票系统

# 地图API密钥 (如果使用第三方地图服务)
VUE_APP_MAP_API_KEY=your_map_api_key_here

# 是否启用调试模式
VUE_APP_DEBUG=true

# 分析和统计 (可选)
VUE_APP_GA_ID=
VUE_APP_SENTRY_DSN=
